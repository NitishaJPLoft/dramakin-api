  "scripts": {
    "copy": "yarn run cpy '.env.example' '.' --rename=.env --no-overwrite ",
    "clean": "rimraf dist",
    "pretty": "prettier ./dist/*.js ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts ./src/**/**/**/**/*.ts  --color --write",
    "build:babel": "NODE_ENV=production babel src --out-dir dist --extensions .ts --source-maps inline",
    "build": "yarn run clean && yarn run build:babel",
    "start": "yarn run build && node dist/index.js",
    "dev:win": "nodemon --watch src -e ts --exec ts-node ./src/index.ts",
    "dev": "yarn run copy && NODE_ENV=development nodemon --watch src -e ts --exec ts-node ./src/index.ts"
  },
